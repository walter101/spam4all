{% extends 'spam4u.base.html.twig' %}

{% block title %}Manage account!{% endblock %}

{% block body %}
    <div class="container">
        <div class="topic-box">
            <h2>We checked contact email addresses</h2>
        </div>
        <div class="content-box">
            <p>
                We fetched the folowing email addresses from your Xmail account:.<br>
            </p>
            {{ dump(insertedEmails) }}
            {% for contactEmail in insertedEmails %}
                {{ contactEmail.email }}<br>
            {% endfor %}
        </div>
    </div>
{% endblock %}